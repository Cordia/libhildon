install-gtkdocpackageDATA: $(gtkdocpackage_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(gtkdocpackagedir)
	@list='$(gtkdocpackage_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo " $(gtkdocpackageDATA_INSTALL) $$d$$p $(DESTDIR)$(gtkdocpackagedir)/$$f"; \
	  $(gtkdocpackageDATA_INSTALL) $$d$$p $(DESTDIR)$(gtkdocpackagedir)/$$f; \
	done

uninstall-gtkdocpackageDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(gtkdocpackage_DATA)'; for p in $$list; do \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo " rm -f $(DESTDIR)$(gtkdocpackagedir)/$$f"; \
	  rm -f $(DESTDIR)$(gtkdocpackagedir)/$$f; \
	done
